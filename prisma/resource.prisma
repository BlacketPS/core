model Resource {
    id               Int       @id @default(autoincrement())
    path             String
    reference        String?   @unique
    createdAt        DateTime  @default(now())
    updatedAt        DateTime  @updatedAt
    banners          Banner[]
    blookBackgrounds Blook[]   @relation("blookBackground")
    blooks           Blook[]   @relation("blook")
    emojis           Emoji[]
    fonts            Font[]
    groups           Group[]
    guilds           Guild[]
    items            Item[]
    packs            Pack[]
    products         Product[]
    userAvatars      User[]    @relation("avatar")
    userBanners      User[]    @relation("banner")
    customAvatars    User[]    @relation("customAvatar")
    customBanners    User[]    @relation("customBanner")
}
